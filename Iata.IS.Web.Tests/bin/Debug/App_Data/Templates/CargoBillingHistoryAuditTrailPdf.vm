                  #if($billingMemo.YourInvoiceBillingMonth != 0)
                    #set($billingMemoYourInvoiceBillingMonthName = $cgoInvManager.GetAbbreviatedMonthName($billingMemo.YourInvoiceBillingMonth))
                  #end
