						#if($downloadPackage)				    
							#if($suppDocs.ContainsKey($billingMemo.Attachments.get_item(0)))
								#set($AssignFolderNumber = $suppDocs.get_item($billingMemo.Attachments.get_item(0))) ## get a value 						
							#else
								#set($CurrentFolderNumber = $CurrentFolderNumber + 1)	
								#set($AssignFolderNumber = $CurrentFolderNumber)							
							#end
							$SuppDocDisplayText$AssignFolderNumber
						#end
						<br />					  
                      #foreach ($attachment in $billingMemo.Attachments)                        		  
						#if($downloadPackage)							
							#if(!$suppDocs.ContainsKey($attachment))
								$suppDocs.Add($attachment , $AssignFolderNumber)	
							#end
