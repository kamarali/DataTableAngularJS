<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <style type="text/css">
    .stageSeparator {
      border-top: 5px solid #888;
      margin: 5px 0px;
    }
    .numeric {
      text-align: right;
    }
    .tableHead {
      font: normal 8pt Arial, Helvetica, sans-serif;
    }
    table.formattedTable {
      background-color: #edeeee;
      border: 1px solid #666;
      padding: 0px 0px 0px 0px;
      margin: 5px 0px 10px 0px;
      border-collapse: collapse;
    }
    table.formattedTable > thead > tr {
      background-color: #d7e9f8;
      color: #000;
      font-weight: bold;
    }
    table.formattedTable > thead > tr > td {
      vertical-align: top;
      text-align: center;
      border: 1px solid #666;
      padding: 8px 3px 8px 3px;
    }
    table.formattedTable > tbody > tr {
      background-color: #fff;
    }
    table.formattedTable > tbody > tr > td {
      padding: 8px 3px 8px 3px;
      font: normal 9pt Arial, Helvetica, sans-serif;
      color: #000000;
      border: 1px solid #666;
    }
  </style>
</head>
<body>
  <div id="content">
    <h2>Cargo Audit Trail Report</h2>
	##CMP508:Audit Trail Download with Supporting Documents
	#set($CurrentFolderNumber = 0) ## -- Numbering Logic, start from 0, increment before using	
	#set($AssignFolderNumber = 0) 
	#set($SuppDocRootPath = 'SUPPDOCS\') ## -- set root path , number will be appended to root path
	#set($SuppDocDisplayText = 'in Folder SUPPDOCS\')
	#set($Slash = '\')
    #if($memoList.Count > 0)
      #set($displayedRMIds = "")
      #foreach ($rejectionMemo in $memoList)
        #if($rejectionMemo.RejectionStage == 3)
          #if($rejectionMemo.Correspondence)
            #set($corr = $rejectionMemo.Correspondence)
            #set($billingMemoCount = $rejectionMemo.RejectionStage + $rejectionMemo.Correspondences.Count + 1)

