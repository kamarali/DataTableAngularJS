                  <td>
                    #if($rejectionMemo.Invoice.InvoiceNumber) $rejectionMemo.Invoice.InvoiceNumber #end
                  </td>
                  <td>
                    #if($rejectionMemo.Invoice.ListingCurrencyDisplayText) $rejectionMemo.Invoice.ListingCurrencyDisplayText #end
                  </td>
                  <td>
                    #if($rejectionMemo.RejectionMemoNumber) $rejectionMemo.RejectionMemoNumber #end
                  </td>
                  <td class="numeric">
                    #if($rejectionMemo.BatchSequenceNumber) $rejectionMemo.BatchSequenceNumber #end
                  </td>
                  <td class="numeric">
                   #if( $rejectionMemo.RecordSequenceWithinBatch)  $rejectionMemo.RecordSequenceWithinBatch #end
                  </td>
                  <td>
                   #if( $rejectionMemo.ReasonCode)  $rejectionMemo.ReasonCode #end
                  </td>
                  <td>
                    #if($rejectionMemo.ISValidationFlag) $rejectionMemo.ISValidationFlag #end
                  </td>
                  <td class="numeric">
                    $rejectionMemo.TotalNetRejectAmount.ToString("N2")
