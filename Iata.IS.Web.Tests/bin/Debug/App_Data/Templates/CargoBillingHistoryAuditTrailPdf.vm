                    <td rowspan="2">
                      $corres.CorrespondenceDate.ToString("dd-MMM-yy")
                    </td>
                    <td rowspan="2">
                      $corres.CorrespondenceNumber.ToString("00000000000")
                    </td>
                    <td rowspan="2">
                     #if( $corres.CorrespondenceStage)  $corres.CorrespondenceStage #end
                    </td>
                    <td rowspan="2">
                      #if($corres.AuthorityToBill) Yes #else No #end
                    </td>
                    <td rowspan="2">
                      #if($corres.Currency.Code) $corres.Currency.Code #end $corres.AmountToBeSettled.ToString("N2")
                    </td>
                  </tr>
                </tbody>
              </table>
              <div>
                <b>Correspondence Details</b>: $corres.CorrespondenceDetails
              </div>
              #if ($corres.Attachments.Count > 0)
                <br />
