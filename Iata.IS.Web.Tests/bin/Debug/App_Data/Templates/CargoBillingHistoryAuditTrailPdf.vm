          #end ##----------------------------------------------------------------------------------------------------------($rejectionMemo.Correspondence) If

          ##******************************************************************************************************************************************REJECTION MEMO 3 START

          #set($Rm3YourInvoiceBillingMonthName = $cgoInvManager.GetAbbreviatedMonthName($rejectionMemo.YourInvoiceBillingMonth))
          <br /><h2>Stage $rejectionMemo.RejectionStage, Rejection Memo</h2>
          <div>
            <table class="formattedTable">
              <thead>
                <tr>
                  <td>
                    Billing Period
                  </td>
                  <td>
                    Billing Member
                  </td>
                  <td>
                    Billed Member
                  </td>
                  <td>
                    Invoice No.
                  </td>
                  <td>
                    Listing Currency
                  </td>
                  <td>
                    Memo No.
                  </td>
                  <td>
                    Batch Seq. No.
                  </td>
                  <td>
