                    <table class="formattedTable">
                      <thead>
                        <tr>
                          <td>
                            Billing Period
                          </td>
                          <td>
                            Billing Member
                          </td>
                          <td>
                            Billed Member
                          </td>
                          <td>
                            Invoice Number
                          </td>
                          <td>
                            Memo No.
                          </td>
                          <td>
                            Batch Seq. No.
                          </td>
                          <td>
                            Reason Code
                          </td>
                          <td>
                            IS - Validation Flag
                          </td>
                          <td>
                            Net Billed Amount
                          </td>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>
                            #if($billingMemo.Invoice.DisplayBillingPeriod) $billingMemo.Invoice.DisplayBillingPeriod #end
