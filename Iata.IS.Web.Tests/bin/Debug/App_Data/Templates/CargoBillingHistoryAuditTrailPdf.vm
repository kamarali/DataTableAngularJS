							#if($suppDocs.ContainsKey($awbRecord.Attachments.get_item(0)))
								#set($AssignFolderNumber = $suppDocs.get_item($awbRecord.Attachments.get_item(0))) ## get a value 						
							#else
								#set($CurrentFolderNumber = $CurrentFolderNumber + 1)	
								#set($AssignFolderNumber = $CurrentFolderNumber)							
