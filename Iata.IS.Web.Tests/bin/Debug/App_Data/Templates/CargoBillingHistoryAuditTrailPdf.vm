						#if($downloadPackage)							
							#if(!$suppDocs.ContainsKey($attachment))
								$suppDocs.Add($attachment , $AssignFolderNumber)	
							#end
							<a href= "$SuppDocRootPath$AssignFolderNumber$Slash$attachment.OriginalFileName">$attachment.OriginalFileName</a> 
						#else
							$attachment.OriginalFileName
						#end
						<br />
                      #end ##----------------------------------------------------------------------------------------------------($attachment in $awbRecord.Attachments) For
                      </div>
                    #end ##---------------------------------------------------------------------------------------------------------($awbRecord.Attachments.Count > 0) If
                  #end ##--------------------------------------------------------------------------------------($awbRecord in $billingMemo.AwbBreakdownRecord) For
                  <div class="stageSeparator"></div>
                #end ##----------------------($billingMemo.CorrespondenceRefNumber == $corr.CorrespondenceNumber) If
              #end ##------------------------($billingMemo in $cargoInvoice.CGOBillingMemo) For
            #end ##--------------------------($CargoInvoice in $billingMemoList) For

